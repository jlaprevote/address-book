<div class="form">
<%= form_for(@contact) do |f| %>
  <ul>
    <% @contact.errors.full_messages.each do |err| %>
      <li><%= err %></li>
    <% end %>
  </ul>

  <p>
    <%= f.label :f_name, 'First Name' %>
    <%= f.text_field :f_name %>
  </p>
  <p>
    <%= f.label :l_name, 'Last Name' %>
    <%= f.text_field :l_name %>
  </p>
  <p>
    <%= f.label :address %>
    <%= f.text_field :address %>
  </p>
  <p>
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </p>
  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit :class => "button" %>
  </p>
<% end %>
</div>
